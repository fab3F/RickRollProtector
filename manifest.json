{
  "name": "RickRollProtector",
  "version": "0.0.1.2",
  "description": "Du wirst nie wieder gerickrolled werden! Wenn du willst, kannst du nach der Warnung nat√ºrlich trotzdem dem Link folgen.",
  "permissions": ["storage","scripting","activeTab","webRequest"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [{
    "js": [ "background.js" ],
    "matches": [ "https://*.youtube.com/*" ],
    "run_at": "document_start"
  }],
  "icons": {
    "256": "icon.png"
  },
  "manifest_version": 3,
  "web_accessible_resources": [{
    "resources": ["error-chrome.html", "error-firefox.html", "background.js", "styles-chrome.css", "styles-firefox.css", "icon.png", "warn.js", "warn.png"],
    "matches": ["<all_urls>"]
  }],
  "browser_specific_settings": {
    "gecko": {
      "id": "konakt@help.info.gf"
    }}
}